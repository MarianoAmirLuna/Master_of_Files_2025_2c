CREATE TEST_BITMAP:V1
TRUNCATE TEST_BITMAP:V1 64
WRITE TEST_BITMAP:V1 0 BLOQUE_UNO_TEST
WRITE TEST_BITMAP:V1 16 BLOQUE_DOS_TEST
WRITE TEST_BITMAP:V1 32 BLOQUE_TRES_TE
WRITE TEST_BITMAP:V1 48 BLOQUE_CUATRO_
FLUSH TEST_BITMAP:V1
TRUNCATE TEST_BITMAP:V1 32
TRUNCATE TEST_BITMAP:V1 64
WRITE TEST_BITMAP:V1 32 NUEVO_BLOQUE_3
WRITE TEST_BITMAP:V1 48 NUEVO_BLOQUE_4
COMMIT TEST_BITMAP:V1
TAG TEST_BITMAP:V1 TEST_BITMAP:V2
WRITE TEST_BITMAP:V2 0 MODIFICADO_V2_
COMMIT TEST_BITMAP:V2
DELETE TEST_BITMAP:V1
READ TEST_BITMAP:V2 0 16
END
